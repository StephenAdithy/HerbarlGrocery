<div class=" reviwewin">
    <MudContainer Class="mt-15">
        <MudText Typo="Typo.h4" Class="mt-5 mb-5 d-flex align-content-center justify-content-center heading">Testimonials</MudText>
        <MudCarousel Class="mud-width-full revtab mb-5" TData="object" ShowArrows="true" ShowBullets="false">
            @{
                var chunkSize = 3;
                var chunks = reviewrating.ChunkBy(chunkSize);
            }

            @foreach (var x in chunks)
            {
                <MudCarouselItem Transition="Transition.Slide">
                    <MudGrid>
                        @foreach (var y in x)
                        {
                            <MudItem xxl="4" xl="4" lg="4" md="4">
                                <MudPaper Class="reviewpaper" Elevation="2">
                                    <MudItem>
                                        <MudText Class="reviewcontent mx-5 my-5" Typo="Typo.subtitle2">@y.Comments</MudText>
                                    </MudItem>
                                    <MudStack Row=true Class="mx-5 my-5">
                                        <MudAvatar Size="Size.Large">
                                            <MudImage Src="@y.img" />
                                        </MudAvatar>
                                        <MudItem>
                                            <MudText Typo="Typo.h6">@y.Name</MudText>
                                            <MudRating SelectedValue="@y.Review" ReadOnly=true Size="Size.Small" />
                                        </MudItem>
                                        <MudSpacer />
                                        <MudImage Src="/img/quotes.png" Class="quotes" />
                                    </MudStack>
                                </MudPaper>
                            </MudItem>
                        }
                    </MudGrid>
                </MudCarouselItem>
            }
        </MudCarousel>

    </MudContainer>

</div>

