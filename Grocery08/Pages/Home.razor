            @foreach (var i in Produts)
            {
                <MudItem md="3" sm="6" xs="12">
                    <MudCard Class="cardhover" Outlined="false" Elevation="0">
                        
                        <MudCardContent Class="text-center">
                            <MudIcon Icon="@Icons.Material.Filled.RemoveRedEye" Color="Color.Dark" Class="eye1" />
